{
  "name": "YouTube Share URL Cleaner",
  "description": "Удаляет параметр 'si' при использовании функции 'Поделиться' на YouTube.",
  "version": "1.6",
  "manifest_version": 3,
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icons/icon128.png"
  },
  "author": "qqrm, gpt and angly",
  "applications": {
    "gecko": {
      "id": "youtube-cleaner@example.com",
      "strict_min_version": "109.0"
    }
  }
}
